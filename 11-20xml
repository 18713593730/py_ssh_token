			<cols>
				<col>
					<name>protocol_id</name>
					<type>int</type>
					<auto_inc></auto_inc>
				</col>
				<col default="NOT NULL">
					<name>name</name>
					<type>varchar</type>
					<type_len>16</type_len>
					<default>""</default>
					<comment>协议名称</comment>
				</col>
			</cols>
			<keys>
				<primary_key>protocol_id</p_key>
				<unique_key indexname="">protocol</u_key>
			</keys>
		</table>
	</database>
