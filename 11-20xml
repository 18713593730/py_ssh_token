<data>
	<records dname="max_vpn_admin" tname="mvad_protocol">
		<record>
			<protocol_id>1</protocol_id>
			<name>http</name>
			<protocol>http</protocol>
			<type>website</type>
			<port>80</port>
		</record>
		<record>
			<protocol_id>2</protocol_id>
