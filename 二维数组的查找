#include <iostream>
#include <vector>

using namespace std;


class Solution
{
    public:
    bool Find(vector<vector<int> > array,int target){
        int row = array.size();
        int col = array[0].size();
        for(int i = row-1; i >= 0; i--){
            for(int j = col-1; j >= 0; j--){
                if(target == array[i][j])
                    return True;
                else if(target > array[i][j]){
                    col = j+1;
                    break;
                }
            }
        }
        return False;
    }
};
